<html>
	<head>
		<title>
			RockLobster
		</title>
	</head>
	<body>
		<div class="blog-title">Tech blog, innit</div>
                <hr />
                {{#plugins}}all-tags||<div>||</div>||<a style="font-size: [occurrences]px" href="category_[tagname].html">||</a>|| &#124; ||13{{/plugins}}
                <hr />
		<div class="blog-posts">
			{{#posts}}
			<div class="blog-posts-entry">
				<div class="blog-posts-entry-title">
					<a href="{{basefilename}}.html">
						{{#metadata}}{{& title}}{{/metadata}}
					</a>
				</div>
				<div class="blog-posts-entry-post">
					{{& post}}
				</div>
			</div>
			<hr />
			{{/posts}}
			<div class="blog-posts-pagination-info">
				Page {{num_pages_current}} of {{num_pages_total}}
			</div>
			<div class="blog-posts-pagination-info-next">
				{{#next_page_relative_url}}<a href="{{next_page_relative_url}}">Next</a>{{/next_page_relative_url}} 
			</div>
			<div class="blog-posts-pagination-info-previous">
				{{#previous_page_relative_url}}<a href="{{previous_page_relative_url}}">Previous</a>{{/previous_page_relative_url}}
			</div>
		</div>
	</body>
</html>
